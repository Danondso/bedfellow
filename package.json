{
  "name": "bedfellow",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "android-dev": "NODE_ENV=dev ENVFILE=.env.dev react-native run-android",
    "android-build-prod": "NODE_ENV=prod ENVFILE=.env.prod react-native build-android --mode=release",
    "ios-build-prod": "NODE_ENV=prod ENVFILE=.env.prod yarn ios --mode Release --scheme release",
    "ios-dev": "NODE_ENV=dev ENVFILE=.env.dev react-native run-ios --scheme development",
    "pods": "cd ios && pod install && cd ../",
    "lint": "eslint .",
    "start": "NODE_ENV=dev ENVFILE=.env.dev react-native start ",
    "server-start": "cd server/ && docker compose up && cd ../",
    "test": "NODE_ENV=jest jest",
    "test-debug": "jest --detectOpenHandles --forceExit",
    "ignore-file-changes": "git update-index --assume-unchanged .env.prod && git update-index --assume-unchanged .env.dev && git update-index --assume-unchanged server/docker-compose.yml && git update-index --assume-unchanged android/gradle.properties",
    "notice-file-changes": "git update-index --no-assume-unchanged .env.prod && git update-index --no-assume-unchanged .env.dev && git update-index --no-assume-unchanged server/docker-compose.yml && git update-index --no-assume-unchanged android/gradle.properties",
    "tsc": "tsc"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "axios": "^1.7.4",
    "cheerio": "^1.0.0-rc.12",
    "fast-fuzzy": "^1.12.0",
    "lottie-ios": "4.4.1",
    "react": "19.1.0",
    "react-content-loader": "^7.0.2",
    "react-native": "0.81.0",
    "react-native-app-auth": "^7.1.0",
    "react-native-config": "1.5.1",
    "react-native-fs": "^2.20.0",
    "react-native-image-colors": "1.5.2",
    "react-native-lottie-splash-screen": "^1.1.2",
    "react-native-paper": "^5.11.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "4.4.0",
    "react-native-svg": "^15.4.0",
    "react-native-vector-icons": "^10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^20.0.1",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.10",
    "@types/react": "^19.1.0",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "@types/ws": "^8.18.1",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.6.3",
    "lint-staged": "^16.1.5",
    "prettier": "^3.3.2",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=20.19.4"
  },
  "packageManager": "yarn@3.6.4",
  "resolutions": {
    "react-native-image-colors@1.2.2": "patch:react-native-image-colors@npm%3A1.2.2#./.yarn/patches/react-native-image-colors-npm-1.2.2-dec6f9506d.patch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
